  <section id="divisions" ng-if="!loading">
    <div class="container divisions-container">
      <div ng-show="league.id != 5" class="col-md-6 text-center main-division-container" ng-repeat="main_division in divisions | filter:{is_main_division:true} | orderBy:'name'">
          <p class="main-division">{{::main_division.name}}</p>
          <div class="small-division" ng-repeat="division in divisions | filter:{is_main_division: false, 'division_id' : main_division.id}">
            {{::main_division.name | uppercase}} {{::division.name | uppercase}} TICKETS
            <div class="small-division-back row">
              <div class="small-division-front">
                <div class="row performer-detail" ng-repeat="performer in division.performers | filter:{division_id:division.id} | orderBy: ['-wins','name']">
                  <div class="league-detail-table hidden-md hidden-lg hidden-xl">
                    <div class="league-detail-image">
                      <a href="{{::performer.url}}"><img src="{{::performer.image_url_medium}}"></a>
                    </div>
                    <div class="league-detail-text">
                        <a class="detail-title" href="{{::performer.url}}">{{::performer.name}}</a>
                        <p class="detail-text">Stadium <a href="{{::performer.venue.url}}">{{::performer.venue.name}}</a></p>
                        <div>
                            <table>
                              <thead>
                                <tr>
                                  <th class="right-cell">
                                    W
                                  </th>
                                  <th class="left-cell">
                                    L
                                  </th>
                                </tr>
                              </thead>
                              <tbody>
                                <tr>
                                  <td class="right-cell">
                                    {{::performer.wins}}<span ng-show="performer.wins === '' || performer.wins === null">-</span>
                                  </td>
                                  <td class="left-cell">
                                    {{::performer.losses}}<span ng-show="performer.losses === '' || performer.wins === null">-</span>
                                  </td>
                                </tr>
                              </tbody>
                            </table>
                          </div>
                    </div>
                  </div>
                    
