  <section id="near-events" ng-show="next_events.length != 0">
    <div class="container">
      <div class="title">UPCOMING GAMES NEAR YOU</div>
      <div ng-repeat="event in next_events | limitTo:4" class="col-md-3 mobile-near-events">
        <a href="{{(event.url + '/' + event.name + '-tickets') | spaceless | lowercase}}">
          <div class="back">
            <div class="front">
              <div class="date-holder">
                <div class="date-inner">
                  <p class="date1">{{event.occurs_at | date:'MMM' : 'UTC' | uppercase}}</p>
                  <p class="date2">{{event.occurs_at | date:'dd' : 'UTC' | uppercase}}</p>
                  <p class="date3">{{event.occurs_at | date:'EEE' : 'UTC' | uppercase}}</p>  
                </div>
              </div>
              <div class="event-holder" ng-show="event.performances[0].primary == true">
                <p class="teams">{{event.performances[1].performer.name.replace('Football', '')}}<span class="at"> at </span><br>{{event.performances[0].performer.name.replace('Football', '')}}</p>
                <p class="event3"><span class="at" ng-if="(event.occurs_at | date:'hh:mm:ss' : 'UTC') == (compareDate | date:'hh:mm:ss' : 'UTC')">TBD</span><span class="at" ng-if="(event.occurs_at | date:'hh:mm:ss' : 'UTC') != (compareDate | date:'hh:mm:ss' : 'UTC')">{{event.occurs_at | date:'h:mm a' : 'UTC' | uppercase}} at</span> <span class="stadium">{{event.venue.name}}</span></p>
                <p class="event4">{{event.available_count}} tickets left</p>
              </div>
              <div class="event-holder" ng-show="event.performances[1].primary == true">
                 <p class="teams">{{event.performances[0].performer.name.replace('Football', '')}}<span class="at"> at </span><br>{{event.performances[1].performer.name.replace('Football', '')}}</p>
                <p class="event3"><span class="at" ng-if="(event.occurs_at | date:'hh:mm:ss' : 'UTC') == (compareDate | date:'hh:mm:ss' : 'UTC')">TBD</span><span class="at" ng-if="(event.occurs_at | date:'hh:mm:ss' : 'UTC') != (compareDate | date:'hh:mm:ss' : 'UTC')">{{event.occurs_at | date:'h:mm a' : 'UTC' | uppercase}} at</span> <span class="stadium">{{event.venue.name}}</span></p>
                <p class="event4">{{event.available_count}} tickets left</p>
              </div>
              <button>Buy Tickets</button>
            </div>
          </div>
        </a>
      </div>
    </div>
  </section>
