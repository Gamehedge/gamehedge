<span id="home" class=leagues>
  <section id="hero" style="background: transparent url({{league.image_url}}) center;background-size:cover;">
    <div id="shader"></div>
    <div id="wrapper">
      <div id="content">
        <div class="container">
          <p class="hidden-md hidden-lg hidden-xl">{{league.name | uppercase}} Tickets with Good Game Guarantee!</p>
          <div id="hero-search" class="">
            <form id="form-home-search" name="formSearch" method="POST" action="/search/">
              <input type="hidden" name="type" value="full" />
              <div class="form-group">
                <img src="<%= image_url "search icon.png" %>">
                <input id="search_element" type="text" class="form-control" placeholder="Search by team, league, game, stadium or player..." ng-model="searchTerm" uib-typeahead="hint.name for hint in getSearchHints($viewValue)" typeahead-template-url="typeahead/bind-node.html" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false">
                 <div class="no-results" ng-show="searchTerm != ''">
                  Not results please try again
                </div>
              </div>
            </form>
          </div>
          <p class="hidden-xs hidden-sm">{{league.name | uppercase}} Tickets with Good Game Guarantee!</p>
        </div>
      </div>
    </div>
  </section>

  <script type="text/ng-template" id="typeahead/bind-node.html">
    <a href="/#{{match.model.url}}" tabindex="-1" ng-bind-html="match.label | uibTypeaheadHighlight:query"></a>
  </script>
