  <section id="hero" style="background: transparent url({{::league.image_url}}) center;background-size:cover;">
    <div id="shader"></div>
    <div id="wrapper">
      <div id="content">
        <div class="container">
          <p class="hidden-md hidden-lg hidden-xl">{{::league.name | uppercase}} Tickets with Good Game Guarantee!</p>

          <p class="hidden-xs hidden-sm" style="margin-top: 20px; margin-bottom: 0px;">{{::league.name | uppercase}} Tickets with Good Game Guarantee!</p>

          <div id="hero-search" class="" style="margin-top: 25px;margin-bottom: 50px;">
            <form id="form-home-search" name="formSearch" onsubmit="function(){return false;};">
              <input type="hidden" name="type" value="full" />
              <div class="form-group">
                <img src="https://d2fj37dggc6l3c.cloudfront.net/assets/images/search icon.png" ng-click="goToSearch()">
