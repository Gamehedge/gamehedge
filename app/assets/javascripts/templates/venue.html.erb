  <section id="about">
    <div class="container">
      <div class="container">
        <div class="page-title venue-about-title">About {{venue.name}}</div>
        <div class="row">
          <div class="col-xs-12 description" bind-unsafe-html="venue.description">
            
          </div>
        </div>
      </div>
    </div>
  </section>
</span>
