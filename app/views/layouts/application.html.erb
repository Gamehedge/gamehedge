<script type="text/javascript">
  var mxpnl_super_props = {};
  var mxpnl_props = {};


  mxpnl_super_props.AdChannel = 'none';
  mxpnl_super_props.AdCmp = 'none';
  mxpnl_super_props.AdGroup = 'none';
  mxpnl_super_props.AdAB = 'none';
  mxpnl_super_props.AdData = 'none';
  mxpnl_super_props.AdData2 = 'none';
  mxpnl_super_props.ScreenResolution = 'not detected';
  
  if (QueryString.adchannel != undefined){
    mxpnl_super_props.AdChannel = QueryString.adchannel;
  }
  if (QueryString.adcmp != undefined){
    mxpnl_super_props.AdCmp = QueryString.adcmp;
  }
  if (QueryString.adgroup != undefined){
    mxpnl_super_props.AdGroup = QueryString.adgroup;
  }
  if (QueryString.adab != undefined){
    mxpnl_super_props.AdAB = QueryString.adab;
  }
  if (QueryString.addata != undefined){
    mxpnl_super_props.AdData = QueryString.addata;
  }
  if (QueryString.addata2 != undefined){
    mxpnl_super_props.AdData2 = QueryString.addata2;
  }
  
  mxpnl_super_props.ScreenResolution = screen.width+'x'+ screen.height;
  mxpnl_props.BrowserWSize = getBrowserWidth()+'x'+ getBrowserHeight();

  mixpanel.register_once(mxpnl_super_props);
  mixpanel.register(mxpnl_props);

