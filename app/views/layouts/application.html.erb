  <link rel="preload" href="https://dynamicvenuemaps.com/maps/css/style_maps.css" as="style" onload="this.rel='stylesheet'">
  <noscript><link rel="stylesheet" href="https://dynamicvenuemaps.com/maps/css/style_maps.css"></noscript>
    
  <%= csrf_meta_tags %>
    
  <style>
    .intro-full {
        width: 100%;
        height: 100%;
        position: fixed;
        z-index: 9999999999999;
        background-color: black;
    }

    .intro-img-container{
        position: absolute;
        width: 300px;
        height: 60px;
        top: 50%;
        left: 50%;
        margin-left: -150px;
        margin-top: -30px;
    }  
  </style>
