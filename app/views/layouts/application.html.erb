    <noscript><link rel="stylesheet" href="https://dynamicvenuemaps.com/maps/css/style_maps.css"></noscript> -->
      
    <%= csrf_meta_tags %>
      
    <style>
      .intro-full {
          width: 100%;
          height: 100%;
          position: fixed;
          z-index: 9999999999999;
          background-color: black;
      }

      .intro-img-container{
          position: absolute;
          width: 300px;
          height: 60px;
          top: 50%;
          left: 50%;
          margin-left: -150px;
          margin-top: -30px;
      }  
    </style>
      <!-- begin olark code -->
  <noscript><a href="https://www.olark.com/site/2065-539-10-9682/contact" title="Contact us" target="_blank">Questions? Feedback?</a> powered by <a href="http://www.olark.com?welcome" title="Olark live chat software">Olark live chat software</a></noscript>
  <!-- end olark code -->
      
      
      <!-- Facebook Pixel Code -->
    
      <noscript><img height="1" width="1" style="display:none"
      src="https://www.facebook.com/tr?id=1221171167912729&ev=PageView&noscript=1" alt="Facebook"
      /></noscript>
      <!-- End Facebook Pixel Code -->
  </head>
      
  <body ng-class="{'no-scrolling': $root.showMenu}" >
    <div class="hidden" ng-controller="MainScreenController">
      <div id="initial_scripts">
        
      </div>
    </div>
    <script type="text/javascript">
      var info_url = "<%= image_url 'icon__info_1x.png' %>";
    </script>
    <%= yield %>
    <p class="notice hidden"><%= notice %></p>
    <p class="alert hidden"><%= alert %></p>
    
    <noscript><img src="//bat.bing.com/action/0?ti=5255950&Ver=2" height="0" width="0" style="display:none; visibility: hidden;" /></noscript>
        
